# "error_occurred"
msgid "error occurred"
msgstr "C'è un errore"

# "range_incorrect"
msgid "Value must be between {low} and {high}"
msgstr "Il valore deve essere compreso tra {low} e {high}"

# "required"
msgid "'{field_label}' field is required"
msgstr "Il campo '{field_label}' è obbligatorio"

# "range_too_high"
msgid "Value must be less than or equal to {high}"
msgstr "Il valore deve essere minore o eguale a {high}"

# "range_too_low"
msgid "Value must be greater than or equal to {low}"
msgstr "Il valore deve essere maggiore o eguale a {low}"

# "unique"
msgid "Duplicate value for {column}"
msgstr "Valore di {column} è duplicato"

# "wrong_value"
msgid "Wrong value"
msgstr "Valore errato"

# "no_match"
msgid "'{value}' does not match"
msgstr "'{value}' non combacia"

# "not_allowed"
msgid "{value} not allowed"
msgstr "{value} non è permesso"

# "field_invalid"
msgid "field is invalid"
msgstr "il campo non è valido"

# "SimpleStr"
msgid "Must be a single line of no more than 255 chars"
msgstr "Deve essere una singola riga con non più di 255 caratteri"

# "NegativeNum"
msgid "Must be a negative number"
msgstr "Deve essere un numero negativo"

# "Zip"
msgid "Zip is not valid"
msgstr "CAP non valido"

# "Email"
msgid "Email is not valid"
msgstr "Email non è valida"

# "Password"
msgid "Must be between 4 and 255 chars"
msgstr "Deve essere tra 4 e 255 caratteri"

# "NotAllDigits"
msgid "Must not be all digits"
msgstr "Non possono essere tutti numeri"

# "IPAddress"
msgid "Not a valid IP address"
msgstr "Indirizzo IP non valido"

# "PositiveInt"
msgid "Must be a positive integer"
msgstr "Deve essere un numero intero positivo"

# "NonEmptyStr"
msgid "Must not be empty"
msgstr "La linea non può essere vuota"

# "State"
msgid "Not a valid state"
msgstr "Non è uno stato valido"

# "StrongPassword"
msgid "Must be between 8 and 255 chars, and contain a non-alpha char"
msgstr "Deve essere tra 8 e 255 caratteri, contenente anche caratteri non alfabetici"

# "NoSpaces"
msgid "Must not contain spaces"
msgstr "Non deve contenere spazi"

# "NegativeInt"
msgid "Must be a negative integer"
msgstr "Deve essere un numero intero negativo"

# "SingleDigit"
msgid "Must be a single digit"
msgstr "Deve essere un numero ad una cifra sola"

# "SingleWord"
msgid "Field must contain a single word"
msgstr "Il campo deve contenere una sola parola"

# "PrintableAndNewline"
msgid "Field contains non-printable characters"
msgstr "Il campo contiene caratteri non stampabili"

# "NonEmptySimpleStr"
msgid "Must be a non-empty single line of no more than 255 chars"
msgstr "Deve essere una linea non vuota con non più di 255 caratteri"

# "Printable"
msgid "Field contains non-printable characters"
msgstr "Il campo contiene caratteri non stampabili"

# "WordChars"
msgid "Must be made up of letters, digits, and underscores"
msgstr "Deve contenere lettere, numeri e trattini bassi"

# "PositiveNum"
msgid "Must be a positive number"
msgstr "Deve essere un numero intero positivo"

# "date_late"
msgid "Date is too late"
msgstr "La data è già trascorsa"

# "date_early"
msgid "Date is too early"
msgstr "La data è troppo in anticipo"

# "datetime_invalid"
msgid "Not a valid DateTime"
msgstr "Non è un valore di data e ora valido"

# "email_format"
msgid "Email should be of the format [_1]"
msgstr "L'email deve essere nel formato [_1]"

#  float_precision1, float_precision2
msgid "May have only one digit after the decimal point, but has {num_digits:num}"
msgid_plural "May have only {precision:num} digits after the decimal point, but has {num_digits:num}"
msgstr[0] "Può esserci solo una cifra dopo il punto decimale, invece ne ha  {num_digits:num}"
msgstr[1] "Può avere solo {precision:num} cifre dopo il punto decimale, invece ne ha {num_digits:num}"

# "float_needed"
msgid "Must be a number. May contain numbers, +, - and decimal separator \'[_1]\'"
msgstr "Deve essere un numero. Può contenre numeri, +, - e separatore decimale \'[_1]\'"

# "float_size"
msgid "Total size of number must be less than or equal to {size}, but is {actual_size}"
msgstr "Il numero delle cifre deve essere minore o uguale a {size}, invece è {actual_size}"

# "integer_needed"
msgid "Value must be an integer"
msgstr "Il numero deve essere intero"

# "money_convert"
msgid "Value cannot be converted to money"
msgstr "Il numero non può essere trasformato in valuta"

# "money_real"
msgid "Value must be a real number"
msgstr "Deve essere un numero reale"

# "integer_positive"
msgid "Value must be a positive integer"
msgstr "Deve essere un intero positivo"

# "select_invalid_value"
msgid "\'[_1]\' is not a valid value"
msgstr "\'[_1]\' non è un valore valido"

# "select_not_multiple"
msgid "This field does not take multiple values"
msgstr "Questo campo non ammette valori multipli"

# "multiple_values_disallowed"
msgid "Field must contain a single value"
msgstr "Il campo deve contenere un valore singolo"

# "text_minlength"
msgid "Field must be at least {minlength} characters. You entered {length}"
msgstr "Il campo deve avere almeno {minlength} caratteri. Tu ne hai inseriti {length}"

# "text_maxlength"
msgid "Field should not exceed {maxlength} characters. You entered {length}"
msgstr "Il campo non può eccedere i {maxlength} caratteri. Tu ne hai inseriti {length}"

# "upload_file_too_big"
msgid "File is too big (> [_1] bytes)"
msgstr "Il file è troppo grande (> [_1] byte)"

# "upload_file_too_small"
msgid "File is too small (< [_1] bytes)"
msgstr "Il file è troppo piccolo (< [_1] bytes)"

# "upload_file_not_found"
msgid "File not found for upload field"
msgstr "Il file non è presente per il caricamento"

# "upload_file_empty"
msgid "File uploaded is empty"
msgstr "Il file caricato è vuoto"

